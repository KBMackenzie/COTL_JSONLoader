      "type": "object",
      "properties": {
        "name": {
          "description": "The name of the override.",
          "$ref": "#/$defs/overrideName"
        },
        "rect": {
          "$ref": "#/$defs/rect"
        },
        "scale": {
          "$ref": "#/$defs/vector2",
          "default": "1, 1"
        },
        "offset": {
          "$ref": "#/$defs/vector2",
          "default": "0, 0"
        }
      },
      "required": ["name", "rect"]
    },
    "rect": {
      "type": "string",
      "pattern": "^ *(\\d+), *(\\d+), *(\\d+), *(\\d+) *$"
    },
    "vector2": {
      "type": "string",
      "pattern": "^ *(\\d+), *(\\d+) *$"
    },
    "colorHex": {
      "type": "string",
      "pattern": "^#(?:[0-9a-fA-F]{3}){1,2}$"
